<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.14-internal"
            xmlns:fx="http://javafx.com/fxml/1">
    <VBox alignment="TOP_CENTER" layoutX="-1.0" layoutY="1.0" prefHeight="600.0" prefWidth="800.0">
        <ImageView fx:id="virus_diagram" fitHeight="402.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true"/>
        <TextArea fx:id="taVirusDescription" prefHeight="200.0" prefWidth="200.0">
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
        </TextArea>
    </VBox>
</AnchorPane>
